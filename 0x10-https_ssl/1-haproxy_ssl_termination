global
    log /dev/log    local0
    stats socket /run/haproxy/admin.sock mode 660 level admin

defaults
    log     global
    mode    http
    option  httplog
    timeout connect  20s
    timeout client  40s
    timeout server  40s

frontend allnellen-in-tech-front
    bind *:80
    bind *:443 ssl crt /etc/haproxy/certs/nell.pem
    http-request redirect scheme https unless { ssl_fc }
    http-request set-header X-Forwarded-Proto https
    default_backend allnellen-in-tech-back

backend allnellen-in-tech-back
    balance roundrobin
    server 107263-web-01 100.26.234.44 
    server 107263-web-02 100.26.153.16  
